{
    "deliveryStreamS3BackupMode": "FailedDocumentsOnly",
    "ebsOptions": {
        "enabled": true,
        "volumeSize": 10
    },
    "enableCloudWatchLogging": true,
    "enableFineGrainedAccess": true,
    "enableVpc": false,
    "indexRotationPeriod": "NoRotation",
    "loggingOptions": {
        "slowSearchLogEnabled": true,
        "appLogEnabled": true,
        "slowIndexLogEnabled": true
