        "dataNodeInstanceType": "t3.small.search",
        "dataNodes": 1,
        "masterNodes": 0,
        "warmNodes": 0
    },
    "subnet": "PUBLIC",
    "vpcName": "default",
    "zoneAwareness": {
        "enabled": false
    }
}